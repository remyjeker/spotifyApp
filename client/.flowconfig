[ignore]
.*/__tests__/.*
.*/build/.*
.*/node_modules/.*
.*/server.js
.*/src/index.js
.*/src/utils/registerServiceWorker.js

[include]

[libs]
flow-typed/*

[lints]
all=warn

[options]
all=true
module.ignore_non_literal_requires=true

[strict]
nonstrict-import
unclear-type
unsafe-getters-setters
untyped-import
untyped-type-import
